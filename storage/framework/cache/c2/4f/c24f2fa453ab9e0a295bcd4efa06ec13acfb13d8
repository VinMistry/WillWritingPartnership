            $this['answer'] = $value1 * $value2;
            $ans = $this['answer'];
            break;
        case '/':
            $this['answer'] = $value1 / $value2;
            $ans = $this['answer'];
            break;
        }
       $result = pg_query($dbconn3,"INSERT INTO Math (value) VALUES ($ans)");
    }
?>
