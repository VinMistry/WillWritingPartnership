        $ans = 0;
      switch($operation) {
        case '+':
            $this['answer'] = $value1 + $value2;
           $ans = $this['answer'];
            break;
        case '-':
            $this['answer'] = $value1 - $value2;
            $ans = $this['answer'];
            break;
